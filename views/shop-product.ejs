    <script>

        async function addtowishlist(productid) {
    try {
        const response = await fetch(`/addToWishlist?id=${productid}`);
       
        if (response.ok) {
            
            
            await Swal.fire({
                title: 'Success',
                text: 'Added to wishlist',
                icon: 'success',
                showConfirmButton: true
            });
            location.reload();
            
        } else {
            
            throw new Error('Failed to add to wishlist');
        }
    } catch (error) {
        console.error('Error:', error.message);
        await Swal.fire({
            title: 'Error',
            text: 'Failed to add to wishlist',
            icon: 'error',
            showConfirmButton: true
        });
    }
}


async function removewishlist(productid) {
    try {
        const response = await fetch(`/removeWishlist?id=${productid}`);
       
        if (response.ok) {
            
           
            await Swal.fire({
                title: 'removed ',
                text: 'removed from wishlist',
                icon: 'success',
                showConfirmButton: true
            });
            location.reload();
            
        } else {
            
            throw new Error('Failed to add to wishlist');
        }
    } catch (error) {
        console.error('Error:', error.message);
        await Swal.fire({
            title: 'Error',
            text: 'Failed to add to wishlist',
            icon: 'error',
            showConfirmButton: true
        });
    }
}


        
        async function addProductCart(productId) {
    try {
        let res = await fetch('/add-to-cart', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({
                productId
            })
        });

        if (res.status === 400) {
            await Swal.fire({
                title: 'Error',
                text: 'Maximum quantity per person for this product has been reached',
                icon: 'error',
                showConfirmButton: true
            });
            
        }
        else if (res.status === 200) { 
            window.location.href = '/cart'; 
        }
    } catch (error) {
        console.error('Error adding product to cart:', error);
       
    }
}
     
    


function searchproduct(category) {
    let text = document.getElementById("search").value;
    const searchInput = document.getElementById('search');
    searchInput.addEventListener('keydown', function(event) {
        if (event.key === 'Enter') {
            location.href=`/shop?category=${category}&search=${text}`
        }
    });
    document.querySelector('#searchResultsCollapse').classList.add('show');
  
    searchInput.addEventListener('blur', function() {
        setTimeout(function() {
            document.querySelector('#searchResultsCollapse').classList.remove('show');
        }, 200);
    });
    // alert(text)
    fetch(`/search?text=${text}&category=${category}`)
    
        .then(response => response.json())
        .then(data => {
            let searchResultDiv = document.getElementById("searchResult");
            searchResultDiv.innerHTML = '';

            if (data.product.length === 0) {
                searchResultDiv.innerHTML = '<p>No products found.</p>';
            } else {
                data.product.forEach(product => {
                    let productDiv = document.createElement("div");
                    productDiv.classList.add("product");

                    productDiv.innerHTML = `
                    <div class='card-body d-flex'>
                    <div>
                        
                            <img src="/uploads/productImages/${product.images[0]}" style="width:50px;" alt="${product.name}">
                    </div>
                    <div>
                        <h6>${product.name}</h6>
                        <p>Price: ${product.price}</p>
                        
                    </div>
                </div> `;

                    searchResultDiv.appendChild(productDiv);
                });
            }
        })
        .catch(error => {
            console.error('Error fetching search results:', error);
        });
}




