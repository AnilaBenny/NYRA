        async function addProductCart(productId) {
            event.preventDefault()
    try {
        let res = await fetch('/add-to-cart', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({
                productId
            })
        });

        if (res.status === 400) {
            await Swal.fire({
                title: 'Error',
                text: 'Maximum quantity per person for this product has been reached',
                icon: 'error',
                showConfirmButton: true
            });
            
        }
        else if (res.status === 200) { 
            
            const Toast = Swal.mixin({
  toast: true,
  position: "bottom", 
  showConfirmButton: false,
  timer: 3000,
  timerProgressBar: true,
  background:"black",
  color:"white",
  didOpen: (toast) => {
    toast.addEventListener('mouseenter', Swal.stopTimer); 
    toast.addEventListener('mouseleave', Swal.resumeTimer); 
  }
});

Toast.fire({
  icon: "success",
  title: "Added to cart"
});

        }
    } catch (error) {
        console.error('Error adding product to cart:', error);
       
    }
    
}
     
async function addtowishlist(productid) {
    try {
        const response = await fetch(`/addToWishlist?id=${productid}`);
        if (response.ok) {
            const Toast = Swal.mixin({
  toast: true,
  position: "bottom", 
  showConfirmButton: false,
  timer: 3000,
  timerProgressBar: true,
  background:"black",
  color:"white",
  didOpen: (toast) => {
    toast.addEventListener('mouseenter', Swal.stopTimer); 
    toast.addEventListener('mouseleave', Swal.resumeTimer); 
  }
});

Toast.fire({
  icon: "success",
  title: "Added to wishlist"
});
            
            const wishlistLink = document.getElementById(`add-to-wishlist-${productid}`);
            wishlistLink.setAttribute('onclick', `removewishlist('${productid}')`);
            wishlistLink.innerHTML = `<i class="fa-solid fa-heart text-danger" id="heart-${productid}"></i>`;
            
        } else {
            throw new Error('Failed to add to wishlist');
        }
    } catch (error) {
        console.error('Error:', error.message);
        await Swal.fire({
            title: 'Error',
            text: 'Failed to add to wishlist',
            icon: 'error',
            showConfirmButton: true
        });
    }
}


async function removewishlist(productid) {
    try {
        const response = await fetch(`/removeWishlist?id=${productid}`);
       
        if (response.ok) {
            
           
            await Swal.fire({
                title: 'removed ',
                text: 'removed from wishlist',
                icon: 'success',
                showConfirmButton: true
            });
            location.reload();
            
        } else {
            
            throw new Error('Failed to add to wishlist');
        }
    } catch (error) {
        console.error('Error:', error.message);
        await Swal.fire({
            title: 'Error',
            text: 'Failed to add to wishlist',
            icon: 'error',
            showConfirmButton: true
        });
    }
}


